{
  "name": "frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:local": "cross-env-shell ANGULAR_APP_ENV=local ng serve",
    "start:dev": "cross-env-shell ANGULAR_APP_ENV=dev ng serve",
    "start:stage": "cross-env-shell ANGULAR_APP_ENV=stage ng serve",
    "start:prod": "cross-env-shell ANGULAR_APP_ENV=prod ng serve",
    "build": "ng build",
    "build:local": "cross-env-shell ANGULAR_APP_ENV=local ng build",
    "build:dev": "cross-env-shell ANGULAR_APP_ENV=dev ng build",
    "build:state": "cross-env-shell ANGULAR_APP_ENV=state ng build",
    "build:prod": "cross-env-shell ANGULAR_APP_ENV=prod ng build",
    "watch": "ng build --watch --configuration development",
    "test": "jest",
    "test:local": "cross-env-shell ANGULAR_APP_ENV=local ng test",
    "test:dev": "cross-env-shell ANGULAR_APP_ENV=dev ng test",
    "test:state": "cross-env-shell ANGULAR_APP_ENV=state ng test",
    "test:prod": "cross-env-shell ANGULAR_APP_ENV=prod ng test",
    "lint": "ng lint",
    "fix": "ng lint --fix"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.3.0",
    "@angular/common": "~13.3.0",
    "@angular/compiler": "~13.3.0",
    "@angular/core": "~13.3.0",
    "@angular/forms": "~13.3.0",
    "@angular/platform-browser": "~13.3.0",
    "@angular/platform-browser-dynamic": "~13.3.0",
    "@angular/router": "~13.3.0",
    "@isnotdefined/eslint-config": "4.4.0",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "wait-on": "^6.0.1",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.3.2",
    "@angular-eslint/builder": "13.2.0",
    "@angular-eslint/eslint-plugin": "13.2.0",
    "@angular-eslint/eslint-plugin-template": "13.2.0",
    "@angular-eslint/schematics": "13.2.0",
    "@angular-eslint/template-parser": "13.2.0",
    "@angular/cli": "~13.3.2",
    "@angular/compiler-cli": "~13.3.0",
    "@isnotdefined/eslint-config": "4.4.0",
    "@isnotdefined/stylelint-config": "^3.2.0",
    "@types/jest": "^27.4.1",
    "@types/node": "^12.20.47",
    "@typescript-eslint/eslint-plugin": "5.17.0",
    "@typescript-eslint/parser": "5.17.0",
    "cross-env": "^7.0.3",
    "cypress": "^9.5.3",
    "eslint": "^8.12.0",
    "jest": "^27.5.1",
    "jest-preset-angular": "^11.1.1",
    "typescript": "~4.6.2"
  }
}
