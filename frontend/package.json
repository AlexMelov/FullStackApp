{
	"name": "frontend",
	"version": "1.0.0",
	"private": true,
	"dependencies":
	{
		"@angular/animations": "13.3.0",
		"@angular/cdk": "13.3.3",
		"@angular/common": "13.3.0",
		"@angular/compiler": "13.3.0",
		"@angular/core": "13.3.0",
		"@angular/forms": "13.3.0",
		"@angular/material": "13.3.3",
		"@angular/platform-browser": "13.3.0",
		"@angular/platform-browser-dynamic": "13.3.0",
		"@angular/router": "13.3.0",
		"harmonize.css": "4.1.4",
		"rxjs": "7.5.0",
		"tslib": "2.3.0",
		"zone.js": "0.11.4",
		"wait-on": "6.0.1"
	},
	"devDependencies":
	{
		"@angular-builders/jest": "13.0.3",
		"@angular-devkit/build-angular": "13.3.2",
		"@angular-eslint/builder": "13.2.0",
		"@angular-eslint/eslint-plugin": "13.2.0",
		"@angular-eslint/eslint-plugin-template": "13.2.0",
		"@angular-eslint/schematics": "13.2.0",
		"@angular-eslint/template-parser": "13.2.0",
		"@angular/cli": "13.3.3",
		"@angular/compiler-cli": "13.3.0",
		"@briebug/jest-schematic": "4.0.0",
		"@cypress/schematic": "1.6.0",
		"@isnotdefined/eslint-config": "4.7.0",
		"@isnotdefined/stylelint-config": "3.2.0",
		"@types/cypress": "1.1.3",
		"@types/jest": "26.0.24",
		"cross-env": "7.0.3",
		"cypress": "9.5.4",
		"eslint": "8.13.0",
		"jest": "27.5.1",
		"jest-preset-angular": "11.1.1",
		"postcss": "8.4.12",
		"postcss-scss": "4.0.3",
		"stylelint": "14.6.1",
		"typescript": "4.6.2"
	},
	"scripts":
	{
		"start": "ng serve",
		"start:local": "npm run start -- --configuration=local",
		"start:dev": "npm run start -- --configuration=dev",
		"start:stage": "npm run start -- --configuration=stage",
		"start:prod": "npm run start -- --configuration=prod",
		"build": "ng build",
		"build:local": "npm run build --configuration=local",
		"build:dev": "npm run build --configuration=dev",
		"build:stage": "npm run build --configuration=stage",
		"build:prod": "npm run build --configuration=prod",
		"test": "ng test",
		"test:local": "npm run test -- --configuration=local",
		"test:dev": "npm run test -- --configuration=dev",
		"test:stage": "npm run test -- --configuration=stage",
		"test:prod": "npm run test -- --configuration=prod",
		"lint": "ng lint && stylelint src/**/**/*.scss --custom-syntax=postcss-scss --cache",
		"fix": "ng lint --fix && stylelint src/**/**/*.scss --custom-syntax=postcss-scss --fix",
		"e2e": "ng e2e",
		"e2e:local": "cross-env-shell CYPRESS_APP_ENV=local npm run e2e",
		"e2e:dev": "cross-env-shell CYPRESS_APP_ENV=dev npm run e2e",
		"e2e:stage": "cross-env-shell CYPRESS_APP_ENV=dev npm run e2e",
		"e2e:prod": "cross-env-shell CYPRESS_APP_ENV=dev npm run e2e",
		"wait-for-serve": "wait-on tcp:4200 tcp:8000 && sleep 30"
	}
}
