{
	"name": "frontend",
	"version": "1.0.0",
	"private": true,
	"dependencies":
	{
		"@angular/animations": "13.3.4",
		"@angular/cdk": "13.3.4",
		"@angular/common": "13.3.4",
		"@angular/compiler": "13.3.4",
		"@angular/core": "13.3.4",
		"@angular/forms": "13.3.4",
		"@angular/material": "13.3.4",
		"@angular/platform-browser": "13.3.4",
		"@angular/platform-browser-dynamic": "13.3.4",
		"@angular/router": "13.3.4",
		"@ngneat/transloco": "4.0.0",
		"harmonize.css": "4.1.4",
		"ngx-crud": "10.1.2",
		"rxjs": "7.5.5",
		"tslib": "2.4.0",
		"zone.js": "0.11.5"
	},
	"devDependencies":
	{
		"@angular-builders/jest": "13.0.4-beta.0",
		"@angular-devkit/build-angular": "13.3.3",
		"@angular-eslint/builder": "13.2.1",
		"@angular-eslint/eslint-plugin": "13.2.1",
		"@angular-eslint/eslint-plugin-template": "13.2.1",
		"@angular-eslint/schematics": "13.2.1",
		"@angular-eslint/template-parser": "13.2.1",
		"@angular/cli": "13.3.3",
		"@angular/compiler-cli": "13.3.4",
		"@briebug/jest-schematic": "4.0.0",
		"@cypress/schematic": "1.6.0",
		"@isnotdefined/eslint-config": "5.0.0",
		"@isnotdefined/stylelint-config": "3.2.0",
		"@krema/angular-eslint-stylelint-builder": "1.2.1",
		"@types/cypress": "1.1.3",
		"@types/jest": "27.4.1",
		"cross-env": "7.0.3",
		"cypress": "9.5.4",
		"eslint": "8.14.0",
		"jest": "27.5.1",
		"jest-preset-angular": "11.1.1",
		"postcss": "8.4.12",
		"postcss-scss": "4.0.3",
		"stylelint": "14.7.1",
		"typescript": "4.6.3",
		"wait-on": "6.0.1"
	},
	"scripts":
	{
		"start": "ng serve",
		"start:local": "npm run start -- --configuration=local",
		"start:dev": "npm run start -- --configuration=dev",
		"start:stage": "npm run start -- --configuration=stage",
		"start:prod": "npm run start -- --configuration=prod",
		"build": "ng build",
		"build:local": "npm run build -- --configuration=local",
		"build:dev": "npm run build -- --configuration=dev",
		"build:stage": "npm run build -- --configuration=stage",
		"build:prod": "npm run build -- --configuration=prod",
		"test": "ng test",
		"lint": "ng lint",
		"fix": "ng lint --fix",
		"e2e": "ng e2e",
		"e2e:local": "cross-env-shell CYPRESS_APP_ENV=local npm run e2e",
		"e2e:dev": "cross-env-shell CYPRESS_APP_ENV=dev npm run e2e",
		"e2e:stage": "cross-env-shell CYPRESS_APP_ENV=stage npm run e2e",
		"e2e:prod": "cross-env-shell CYPRESS_APP_ENV=prod npm run e2e",
		"wait-for-serve": "wait-on tcp:4200 tcp:8000 && sleep 30"
	}
}
