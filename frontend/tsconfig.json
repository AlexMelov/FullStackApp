{
	"extends": "../tsconfig.json",
	"compilerOptions":
	{
		"target": "ES6",
		"skipLibCheck": true,
		"types":
		[
			"cypress"
		],
		"lib":
		[
			"dom",
			"dom.iterable",
			"esnext",
			"ES6"
		],
		"outDir": "./build",
		"resolveJsonModule": true,
		"allowSyntheticDefaultImports": true,
		"jsx": "react-jsx"
	},
	"exclude":
	[
		"./node_modules"
	],
	"include":
	[
		"./src",
		"**/*.ts"
	]
}


