{
	"extends":
	[
		"@isnotdefined/eslint-config"
	],
	"rules":
	{
		"no-console":
		[
			"error",
			{
				"allow":
				[
					"table"
				]
			}
		]
	},
	"overrides":
	[
		{
			"files":
			[
				"*.ts"
			],
			"extends":
			[
				"plugin:@angular-eslint/template/process-inline-templates"
			],
			"parserOptions":
			{
				"project":
				[
					"./tsconfig.lint.json"
				]
			}
		},
		{
			"files":
			[
				"*.html"
			],
			"extends":
			[
				"plugin:@angular-eslint/template/recommended"
			]
		}
	]
}
