{
	"version": 2,
	"builds": [
		{
			"src": "./build/src/index.js",
			"use": "@vercel/node",
			"config":
			{
				"includeFiles":
				[
					"./build/**"
				]
			}
		},
		{
			"src": "../frontend/build/src/index.html",
			"use": "@vercel/static",
			"config":
			{
				"includeFiles":
				[
					"../frontend/build/**"
				]
			}
		}
	],
	"routes":
	[
		{
			"src": "/api",
			"dest": "./build/src/index.js"
		},
		{
			"src": "/",
			"dest": "../frontend/build/index.html"
		}
	]
}
