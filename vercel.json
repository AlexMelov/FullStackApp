{
	"version": 2,
	"builds":
	[
		{
			"src": "backend/build/**",
			"use": "@vercel/node"
		},
		{
			"src": "frontend/build/**",
			"use": "@vercel/static"
		}
	],
	"routes":
	[
		{
			"src": "/api/(.*)",
			"dest": "backend/build/src/index.js"
		},
		{
			"src": "/",
			"dest": "frontend/build/index.html"
		},
		{
			"src": "/(.+)",
			"dest": "frontend/build/$1"
		}
	]
}

