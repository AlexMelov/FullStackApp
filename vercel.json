{
	"version": 2,
	"builds": [
		{
			"src": "./backend/build/src/index.js",
			"use": "@vercel/node",
			"config":
			{
				"includeFiles":
				[
					"./backend/build/**"
				]
			}
		},
		{
			"src": "./frontend/build/index.html",
			"use": "@vercel/static",
			"config":
			{
				"includeFiles":
				[
					"./frontend/build/**"
				]
			}
		}
	],
	"rewrites": [
		{
			"source": "/api",
			"destination": "backend/build/src/index.js"
		},
		{
			"source": "/",
			"destination": "./frontend/build/index.html"
		}
	]
}
