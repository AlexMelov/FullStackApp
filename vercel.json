{
	"version": 2,
	"builds":
	[
		{
			"src": "backend/build/index.js",
			"use": "@vercel/node",
			"config":
			{
				"includeFiles":
				[
					"backend/build/src/**"
				]
			}
		},
		{
			"src": "frontend/dist/frontend/**",
			"use": "@vercel/static"
		}
	],
	"routes":
	[
		{
			"src": "/api/(.*)",
			"dest": "backend/build/index.js"
		},
		{
			"src": "/",
			"dest": "frontend/dist/frontend/$1"
		},
		{
			"src": "/todos",
			"dest": "frontend/dist/frontend/$1"
		},
		{
			"src": "/login",
			"dest": "frontend/dist/frontend/$1"
		},
		{
			"src": "/register",
			"dest": "frontend/dist/frontend/$1"
		},
		{
			"src": "/(.+)",
			"dest": "frontend/dist/frontend/$1"
		}
	]
}

