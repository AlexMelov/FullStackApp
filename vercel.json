{
	"version": 2,
	"env": {
	},
	"builds": [
		{
			"src": "backend/build/src/index.js",
			"use": "@vercel/node",
			"config":
			{
				"includeFiles":
				[
					"backend/build/**"
				]
			}
		},
		{
			"src": "frontend/build/src/index.html",
			"use": "@vercel/static",
			"config":
			{
				"includeFiles":
				[
					"frontend/build/**"
				]
			}
		}
	],
	"routes":
	[
		{
			"src": "/api",
			"dest": "backend/build/src/index.js"
		},
		{
			"src": "/",
			"dest": "frontend/build/index.html"
		}
	]
}
