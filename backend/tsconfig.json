{
	"compilerOptions":
	{
		"outDir": "./build",
		"target": "es5",
		"module": "esnext",
		"moduleResolution": "node",
		"allowSyntheticDefaultImports": true,
		"esModuleInterop": true,
		"allowJs": true,
		"skipLibCheck": true
	},
	"exclude":
	[
		"./node_modules"
	],
	"include":
	[
		"./src"
	]
}
