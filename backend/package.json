{
	"name": "backend",
	"version": "1.0.0",
	"private": true,
	"type": "module",
	"dependencies": {
		"cors": "^2.8.5",
		"cross-env": "7.0.3",
		"dotenv": "^16.0.0",
		"express": "^4.17.3",
		"mongodb": "^4.4.1",
		"mongoose": "^6.2.7",
		"wait-on": "^6.0.1",
		"web-vitals": "^2.1.4"
	},
	"devDependencies": {
		"@isnotdefined/eslint-config": "4.3.0",
		"@types/body-parser": "^1.19.2",
		"@types/express": "^4.17.13",
		"@types/jest": "^26.0.24",
		"@types/node": "^17.0.21",
		"cross-env": "7.0.3",
		"eslint": "8.11.0",
		"jest": "^26.6.3",
		"kill-port-process": "3.0.1",
		"prettier": "2.6.0",
		"supertest": "^6.2.2",
		"ts-jest": "^26.5.6",
		"ts-node": "^10.7.0",
		"typescript": "4.5.5"
	},
	"scripts": {
		"start": "node --loader ts-node/esm --require dotenv/config src/index.ts",
		"start:local": "cross-env-shell EXPRESS_APP_ENV=local npm run start",
		"start:dev": "cross-env-shell EXPRESS_APP_ENV=dev npm run start",
		"start:stage": "cross-env-shell EXPRESS_APP_ENV=stage npm run start",
		"start:prod": "cross-env-shell EXPRESS_APP_ENV=prod npm run start",
		"build": "tsc",
		"build:local": "cross-env-shell EXPRESS_APP_ENV=local npm run build",
		"build:dev": "cross-env-shell EXPRESS_APP_ENV=dev npm run build",
		"build:stage": "cross-env-shell EXPRESS_APP_ENV=stage npm run build",
		"build:prod": "cross-env-shell EXPRESS_APP_ENV=prod npm run build",
		"lint": "eslint \"src/**/*.ts\" && eslint \"src/**/**/*.ts\"",
		"fix": "eslint \"src/**/*.ts\" --fix && eslint \"src/**/**/*.ts\" --fix",
		"test": "jest"
	}
}
