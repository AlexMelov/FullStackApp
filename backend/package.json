{
	"name": "backend",
	"version": "1.0.0",
	"private": true,
	"type": "module",
	"dependencies": {
		"express": "^4.17.3",
		"mongoose": "^6.2.4",
		"web-vitals": "^2.1.4",
		"typescript": "4.5.4",
		"cross-env": "7.0.3"
	},
	"devDependencies": {
		"@types/body-parser": "^1.19.2",
		"@types/express": "^4.17.13",
		"@types/mongoose": "^5.11.97",
		"@types/node": "^17.0.21",
		"eslint": "^8.10.0",
		"eslint-config-redaxmedia": "3.4.0",
		"prettier": "2.5.1",
		"ts-node": "^10.7.0"
	},
	"scripts": {
		"start": "node --loader ts-node/esm --require dotenv/config src/index.ts",
		"start:dev": "cross-env-shell EXPRESS_APP_ENV=dev npm run start",
		"start:stage": "cross-env-shell EXPRESS_APP_ENV=stage npm run start",
		"start:prod": "cross-env-shell EXPRESS_APP_ENV=prod npm run start",
		"build": "tsc",
		"lint": "eslint src/**/*.ts --cache",
		"fix": "eslint src/**/*.ts --fix"
	}
}
