{
	"name": "backend",
	"version": "1.0.0",
	"private": true,
	"type": "module",
	"dependencies": {
		"cross-env": "7.0.3",
		"dotenv": "^16.0.0",
		"express": "^4.17.3",
		"mongodb": "^4.4.1",
		"mongoose": "^6.2.7",
		"web-vitals": "^2.1.4"
	},
	"devDependencies": {
		"@types/body-parser": "^1.19.2",
		"@types/express": "^4.17.13",
		"@types/node": "^17.0.21",
		"eslint": "8.11.0",
		"eslint-config-redaxmedia": "4.2.1",
		"prettier": "2.6.0",
		"ts-node": "^10.7.0",
		"typescript": "^4.6.2"
	},
	"scripts": {
		"start": "node --loader ts-node/esm --require dotenv/config src/index.ts",
		"start:dev": "cross-env-shell EXPRESS_APP_ENV=dev npm run start",
		"start:stage": "cross-env-shell EXPRESS_APP_ENV=stage npm run start",
		"start:prod": "cross-env-shell EXPRESS_APP_ENV=prod npm run start",
		"build": "tsc",
		"lint": "eslint \"src/**/*.ts\"",
		"fix": "eslint \"src/**/*.ts\" --fix"
	}
}
